<?xml version='1.0' encoding='utf-8'?>
<widget id="com.mupal.app" version="2.0.10" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>MUPal</name>
    <description>
        An Android application to calculate CGPA, GPA, Percentage of a student, according to Mumbai University rules and regulations.
    </description>
    <author email="frankobase98@gmail.com" href="">
        frankobase
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="orientation" value="portrait" />
    <preference name="StatusBarOverlaysWebView" value="true" />
    <preference name="StatusBarBackgroundColor" value="#53538a" />
</widget>
